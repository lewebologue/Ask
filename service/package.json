{
  "name": "ask_service",
  "version": "1.0.0",
  "description": "Service for Ask application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_DEBUG=https node ./dist/server.js",
    "start:dev": "cross-env PORT=3005 NODE_ENV=dev NODE_OPTIONS='--inspect=6002' node ./dist/server.js",
    "watch": "npm run build && (tsc -p tsconfig.json -watch | cross-env PORT=3005 nodemon --inspect=6001 ./dist/server.js)",
    "watch:tsc": "npm run clean && npm run copyToDist && cross-env PORT=3005 NODE_ENV=dev tsc-watch --noClear -p tsconfig.json --onSuccess \"npm run start:dev\"",
    "lint": " npx eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ./**/*.ts",
    "doc": "jsdoc -c jsdoc.json",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent=false",
    "testOne": "jest -i tests/services/user.test",
    "build": "npm run clean && tsc -p tsconfig.json && npm run copyToDist",
    "copyToDist": "copyfiles --all \"./public/**\" \"./dist/\"",
    "clean": "rimraf dist",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "tsc": "npx tsc -p tsconfig.json",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n GreenWebAdvisor",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migrate:rollback": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add",
      "tslint"
    ]
  },
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "@services": "dist/core/services/",
    "@middleware": "dist/middleware/",
    "@model": "dist/core/model/",
    "@repository": "dist/data/repository/",
    "@entities": "dist/data/entities/",
    "@mappers": "dist/data/mappers/",
    "@utils": "dist/utils/",
    "@lib": "dist/lib/"
  },
  "dependencies": {
    "@types/uuid": "9.0.7",
    "app-root-path": "3.1.0",
    "axios": "1.6.2",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "cheerio": "1.0.0-rc.12",
    "copyfiles": "2.4.1",
    "cors": "2.8.5",
    "crypto-js": "4.2.0",
    "express": "4.18.2",
    "express-json-validator-middleware": "3.0.1",
    "express-jwt": "8.4.1",
    "helmet": "7.1.0",
    "inflight": "1.0.6",
    "js-yaml": "4.1.0",
    "jsdom": "23.0.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "module-alias": "2.2.3",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "parse-json": "8.1.0",
    "puppeteer": "21.5.2",
    "sinon": "17.0.0",
    "typeorm": "0.3.17",
    "uuid": "9.0.1",
    "validator": "13.11.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.8",
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.17",
    "@types/crypto-js": "4.2.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.10",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash": "4.14.202",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.11",
    "@types/node": "20.10.2",
    "@types/sinon": "17.0.2",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "cross-env": "7.0.3",
    "eslint": "8.55.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "4.0.2",
    "lint-staged": "15.1.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-mock-imports": "1.3.8",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.2"
  }
}
